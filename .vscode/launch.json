{
  "version": "0.2.0",
  "configurations": [{
    "type": "node",
    "request": "launch",
    "name": "Launch Program",
    "runtimeArgs": ["--no-lazy", "-r", "ts-node/register", "-r", "tsconfig-paths/register"],
    "args": ["${file}"],
    "cwd": "${workspaceFolder}",
    "protocol": "inspector",
    "sourceMaps": true,
    "console": "integratedTerminal"
  }]
}